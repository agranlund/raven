:FF  /* offset + 0x38  not used */
:00  /* offset + 0x39 RIGHT ALT GR (Atari has just ALT) */
:FF  /* offset + 0x3A <,> (M) */
:FF  /* offset + 0x3B J */
:FF  /* offset + 0x3C U */
:27  /* offset + 0x3D 7 */
:27  /* offset + 0x3E 8 */
:FF  /* offset + 0x3F F8 */
:FF  /* offset + 0x40  not used */
:FF  /* offset + 0x41 <;> (,) */
:FF  /* offset + 0x42 K */
