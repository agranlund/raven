:FF  /* offset + 0x22 X */
:FF  /* offset + 0x23 D */
:FF  /* offset + 0x24 E */
:FF  /* offset + 0x25 4 */
:FF  /* offset + 0x26 3 */
:FF  /* offset + 0x27 F5 */
:FF  /* offset + 0x28  not used */
